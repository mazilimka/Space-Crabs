config_version=5

[application]

config/name="Space Crabs"
config/version="0.1"
run/main_scene="res://Main/main.tscn"
run/enable_alt_space_menu=true
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=120
boot_splash/bg_color=Color(0.259294, 0.259294, 0.259294, 1)
boot_splash/image="res://Assets/Spaceships/space_ship.png"
boot_splash/use_filter=false
config/icon="res://Assets/Planets/planet-saturn-pixel-art-png.png"
boot_splash/minimum_display_time=5000

