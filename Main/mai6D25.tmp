@onready var coin := preload(\"res://Coin/Coin.tscn\")


func _ready():
	var hight := 5
	var width := 5
	#var coin_spawner := global_position
	var offset_x := 40
	var offset_y := 50
	for y_pos in range(hight):
		for x_pos in range(width):
			var x_coin_inst : Node2D = coin.instantiate()
			add_child(x_coin_inst, true)
			x_coin_inst.owner = get_parent()
			x_coin_inst.global_position = global_position + Vector2(offset_x * x_pos, offset_y * y_pos)
		
		
"

